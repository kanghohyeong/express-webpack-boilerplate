{
  "name": "express-webpack-boilerplate",
  "version": "0.0.0",
  "private": true,
  "nodemonConfig": {
    "ignore": [
      "public/*"
    ]
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon ./bin/www",
    "build": "webpack --config webpack.prod.js",
    "server": "cross-env NODE_ENV=production node ./bin/www"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.4",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.2.2",
    "sass": "^1.39.0",
    "sass-loader": "^12.1.0",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-middleware": "^5.0.0",
    "webpack-merge": "^5.8.0"
  }
